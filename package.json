{
  "name": "google-sheets-tabs-on-top",
  "version": "0.0.0",
  "description": "Move Google Sheets tabs on top",
  "main": "",
  "scripts": {
    "dev": "webextension-toolbox dev",
    "build": "tsc && webextension-toolbox build",
    "lint": "eslint ./app/scripts/**/*",
    "lint:fix": "eslint ./app/scripts/**/* --fix",
    "supported-browsers": "yarn browserslist",
    "archive": "git archive HEAD -o storage/source.zip",
    "type-check": "tsc",
    "preinstall": "typesync || :"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yamadashy/google-sheets-tabs-on-top.git"
  },
  "keywords": [
    "google",
    "sheets",
    "tab",
    "documents"
  ],
  "author": "yamadashy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamadashy/google-sheets-tabs-on-top/issues"
  },
  "dependencies": {
    "core-js": "3.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/babel__core": "^7.1.7",
    "@types/babel__preset-env": "^7.9.0",
    "@types/chrome": "^0.0.122",
    "@types/core-js": "2.5.3",
    "@types/eslint": "^7.2.0",
    "@types/jquery": "^3.3.32",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.0.6",
    "eslint": "^7.5.0",
    "fork-ts-checker-webpack-plugin": "^5.0.11",
    "typescript": "^3.7.5",
    "typesync": "^0.7.0",
    "webextension-toolbox": "^3.0.0",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "engines": {
    "node": ">=10",
    "yarn": "^1.18.0"
  }
}
